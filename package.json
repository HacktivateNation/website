{
  "dependencies": {
    "autoprefixer": "^10.4.15",
    "eleventy-plugin-icons": "^4.1.1",
    "lucide-static": "^0.276.0",
    "material-icons": "^1.13.11",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3"
  },
  "scripts": {
    "copy:assets": "cp -r images _site/images && cp -r js _site/js && cp -r css/lib _site/css/lib",
    "build:css": "postcss css/styles.css -o _site/css/styles.css",
    "build": "npx eleventy && npm run build:css && npm run copy:assets"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "postcss-cli": "^10.1.0"
  },
  "name": "hacktivate-eleventy",
